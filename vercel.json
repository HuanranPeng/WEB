{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },

    {
      "src": "/2026/sitemap-trice-2025\\.xml$",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8",
        "Cache-Control": "public, max-age=86400"
      },
      "continue": true
    },
    {
      "src": "/2026/robots\\.txt$",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8"
      },
      "continue": true
    },

    { "src": "/2023/?$", "status": 302, "headers": { "Location": "https://huanran-portfolio.webflow.io/2026" } },
    { "src": "/2023/(.*)", "status": 302, "headers": { "Location": "https://huanran-portfolio.webflow.io/2026/$1" } },

    { "src": "/2026/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/2026/images/(.*)", "dest": "/images/$1" },
    { "src": "/2026/meta/(.*)", "dest": "/meta/$1" },
    { "src": "/2026/favicon/(.*)", "dest": "/favicon/$1" },
    { "src": "/2026/favicon.svg", "dest": "/favicon.svg" },

    { "src": "/2026(?:/.*)?", "dest": "/index.html" },

    { "src": "/(?!2026(?:/|$)).*", "status": 404, "dest": "/404.html" }
  ]
} 